{
  "projectName": "Lofi",
  "languages": [
    {
      "id": "lofi",
      "grammar": "src/lofi.langium",
      "fileExtensions": [".lofi"],
      "textMate": {
        "out": "syntaxes/lofi.tmLanguage.json"
      },
      "monarch": {
        "out": "syntaxes/lofi.monarch.ts"
      },
      "prism": {
        "out": "syntaxes/lofi.prism.js"
      },
      "bnf": {
        "out": "syntaxes/lofi.ebnf",
        "dialect": "EBNF"
      },
      "chevrotainParserConfig": {
        "recoveryEnabled": true,
        "nodeLocationTracking": "full"
      }
    }
  ],
  "out": "src/generated",
  "mode": "development"
}
